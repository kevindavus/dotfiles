{
  "manifest_version": 2,
  "name": "AdNauseam",
  "update_url": "https://rednoise.org/adnauseam/updates.xml",
  "version": "3.7.802",
  "default_locale": "en",
  "description": "__MSG_extShortDesc__",
  "icons": {
    "16": "img/icon_16.png",
    "128": "img/icon_128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/browsericons/icon19.png",
      "38": "img/browsericons/icon38.png"
    },
    "default_title": "AdNauseam",
    "default_popup": "menu.html"
  },
  "author": "Daniel C. Howe",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["js/vapi.js", "js/vapi-client.js", "js/adn/textads.js", "js/adn/parser.js", "js/contentscript.js"],
    "run_at": "document_start",
    "all_frames": true
  }, {
    "matches": ["http://*/*", "https://*/*"],
    "js": ["js/scriptlets/subscriber.js"],
    "run_at": "document_idle",
    "all_frames": false
  }],
  "commands": {
      "launch-element-zapper": {
          "description": "__MSG_popupTipZapper__"
      },
      "launch-element-picker": {
          "description": "__MSG_popupTipPicker__"
      },
      "launch-logger": {
          "description": "__MSG_popupTipLog__"
      }
  },
  "incognito": "split",
  "minimum_chrome_version": "38.0",
  "options_page": "dashboard.html",
  "options_ui": {
    "page": "options_ui.html"
  },
  "permissions": [
    "contextMenus",
    "privacy",
    "storage",
    "tabs",
    "unlimitedStorage",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "management"
  ],
  "short_name": "AdNauseam",
  "storage": {
    "managed_schema": "managed_storage.json"
  }
}
